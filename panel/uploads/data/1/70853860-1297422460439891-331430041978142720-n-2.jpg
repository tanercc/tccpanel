<script>
    $(document).ajaxSend(function(event, request, settings) {
        request.setRequestHeader("X-Requested-With", "XMLHttpRequest");
    });
    $("a.sidebar-toggle").on("click", function() {
        var sc = (localStorage.getItem('sidebar-collapse') == 'true') ? 'false' : 'true';
        localStorage.setItem('sidebar-collapse', sc);
        console.log(localStorage.getItem('sidebar-collapse'))
    });
    $(document).ready(function() {
        $('.summernote').summernote({
            lang: 'tr-TR', // default: 'en-US'
            height: 200,
            callbacks: {
                onImageUpload: function(image) {
                    uploadSummerImage(image[0], this);
                }
            }
        });

        function uploadSummerImage(image, editor) {
            var data = new FormData();
            data.append("image", image);
            $.ajax({
                url: '/panel/tr/upload',
                cache: false,
                contentType: false,
                processData: false,
                data: data,
                type: "post",
                success: function(data) {
                    if (data.success) {
                        var image = $('<img>').attr('src', data.result);
                        $(editor).summernote("insertNode", image[0]);
                    } else {
                        alert(data.result);
                    }
                },
                error: function(data) {
                    console.log(data);
                }
            });
        }

        function uploadImage(image) {
            var result = "";
            var data = new FormData();
            data.append("page", 'page');
            data.append("id", '1');
            data.append("image", image);
            $.ajax({
                url: '/panel/tr/upload',
                cache: false,
                contentType: false,
                processData: false,
                data: data,
                type: "post",
                success: function(data) {
                    if (data.success) {
                        result = data.result;
                    } else {
                        alert(data.result);
                    }
                },
                error: function(data) {
                    console.log(data);
                }
            });
            return result;
        }

        function uploadFile(file) {
            var result = "";
            var data = new FormData();
            data.append("page", 'page');
            data.append("id", '1');
            data.append("file", file);
            $.ajax({
                url: '/panel/tr/upload',
                cache: false,
                contentType: false,
                processData: false,
                data: data,
                type: "post",
                success: function(data) {
                    if (data.success) {
                        result = data.result;
                    } else {
                        alert(data.result);
                    }
                },
                error: function(data) {
                    console.log(data);
                }
            });
            return result;
        }

        if (localStorage.getItem('sidebar-collapse') == 'true') {
            $("body").addClass("sidebar-collapse");
        }
    });
</script>

