    }
});
$(".langform").submit(function(e){
    e.preventDefault();
    if(!this.checkValidity()) {
        this.reportValidity();
        return;
    }
    $.ajax({
        url: this.action,
        type: this.method,
        data: $(this).serialize(),
        beforeSend: function() {
            $('.fa.fa-spinner').show();
        }
    }).done(function() {
        console.log('OK');
        $('.fa.fa-spinner').hide();
    }).fail(function() {
        console.log('ER');
        $('.fa.fa-spinner').hide();
    });
});
