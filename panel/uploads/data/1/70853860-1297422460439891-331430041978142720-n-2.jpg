                pagination: {
                    more: (params.page * 10) < data.count
                }
            };
        }
    }
});$('#upper_id').select2({
    minimumResultsForSearch: Infinity,
    ajax: {
        url: "/panel/tr/page/select2",
        dataType: 'json',
        type: "POST",
        data: function (params) {
            return {
                search: params.term,
                page: params.page,
                id: 1
            };
        },
        processResults: function (data, params) {
